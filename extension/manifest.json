{
  "manifest_version": 3,
  "name": "fifaaddict",
  "version": "0.001",
  "content_scripts": [
    {
      "matches": ["https://en.fifaaddict.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["scripting", "tabs"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "background": {
    "service_worker": "background.js"
  }
}
